.container {
  height: 100%;
  margin: 0 auto;
  align-items: center;
  width: 100%;
  box-sizing: border-box;
  font-family:"Microsoft Yahei";
 

 
} 
image{width: 100%;display: block}
.cler{clear: both}
 .dt{width: 100%;height: 110rpx;} 
.db{height:110rpx;background: #fff; position: fixed;bottom:0;z-index:100;width:100%;  font-family: "Microsoft YaHei";border-top: 1px solid #dcdcdc;justify-content: space-between;display: flex}
.db .navigator{float: left; text-align: center;font-size: 0.8125rem;color: #666; height: 130rpx;flex: 1;display: block}
 .on{color: #af2a33 !important; background: #fff} 
.db .navigator .db_img{width: 40rpx;height: 60rpx;margin: 0 auto;position: relative;overflow: hidden}
.db .navigator image{display: block;width:38rpx;height:34rpx; margin: 0.5rem auto 0.2rem;position: absolute;top: 0;left: 0}
.dett{color: #961010}