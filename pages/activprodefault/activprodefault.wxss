.sultationnaw{width: 650rpx;margin: 0 auto;display: flex;justify-content: space-between;padding: 40rpx 0}
.sultationnaw .sula_t{width: 100rpx;font-size: 0.64rem;text-align: center;color: #110f10;font-weight: bold}
.sultationnaw .sula_t image{height: 62rpx;width: 62rpx;margin: 0 auto;margin-bottom: 10rpx;}
.sultationnaw > image{width: 143rpx}

.sultaview{width: 690rpx;margin: 0 auto}

.sultaview_t{width: 100%;padding-bottom: 35rpx}
.sultaview_t text:first-of-type{font-size: 40rpx;color: #131112;font-weight: bold;display: block;padding-top: 40rpx}
.sultaview_t text:nth-of-type(2){font-size: 35rpx;color: #131112;font-weight: bold;display: block;padding: 15rpx 0}
/* .sultaview_t text:nth-of-type(3){} */
.sultaview_t text:nth-of-type(3){font-size: 35rpx;color: #131112;font-weight: bold;display: block}
.sultaview_t rich-text{font-size: 25rpx;color: #666;display: block;padding: 15rpx 0}
.pactivpro{width: 100%;background: #ededed;margin-bottom: 1.5rem}
.pactivpro .actititle{font-size: 30rpx;color: #110f10;font-weight: bold;padding: 15rpx 0;padding-left: 30rpx;box-sizing: border-box;border-bottom: 2px solid #ffffff}

.productview{width: 690rpx;display: flex;margin: 0 auto;justify-content: space-between;padding: 20rpx 0}
.productview image{width: 196rpx;height: 196rpx;border-radius: 20rpx}

.sultaview_c{width: 100%;position: relative;}
.sultaview_c .lqu{width: 100%;height: 100%;position: absolute;left: 0;top: 0;background: rgba(0, 0, 0, 0.3);font-size: 15rpx;color: #fff;box-sizing: border-box;padding-top: 140rpx;padding-left: 600rpx}
.sultaview_c .lqu .lqu1{width: 60rpx;text-align: center;margin-bottom: 20rpx}
.sultaview_c .lqu image{height: 30rpx;width: 30rpx;margin: 10rpx auto}